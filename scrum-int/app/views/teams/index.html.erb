<h1>Team List</h1>

<table>
  <tr>
    <th>Team Name</th>
    <th>Team Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
<% @teams.each do |team| %>
  <tr>
    <td><%= team.name %></td>
    <td><%= team.email %></td>
    <td><%= link_to 'Show', team %></td>
    <td><%= link_to 'Edit', edit_team_path(team) %></td>
    <td><%= link_to 'Destroy', team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
</table>

<br />

<%= link_to 'Add New Team', new_team_path %>